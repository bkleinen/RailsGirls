<script type="text/javascript">
  form = {
    "action" : "/registrations",
    "method" : "post",
    "html": []
  }
  structure_to_render = <%= @form.structure.html_safe %>;
  structure_to_render.push({
      "type" : "submit",
      "value" : "Register"
  });
  structure_to_render.push({
    "type": "hidden",
    "name": "authenticity_token",
    "value": "<%= form_authenticity_token %>"
  });
  structure_to_render.push({
    "type": "hidden",
    "name": "form_id",
    "value": "<%= @form.id %>"
  });
</script>
<form id="form_preview"></form>